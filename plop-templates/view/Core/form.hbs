import $i18n from '@/modules/FPKG-230000-BackendCore/lang';

export default {
  {{#each form}}
  {{this.key}}: {
    label: '{{properCase ../ROUTER_FIRST}}.{{this.key}}',
    id: '{{this.id}}',
    searchId: 'search_{{this.id}}',
    {{#if this.required}}
    rules: {
      required: true,
      message: $i18n.global.t('Util.isRequired', {
        label: $i18n.global.t('{{properCase ../ROUTER_FIRST}}.{{this.key}}'),
      }),
    },
    {{/if}}
  },
  {{/each}}
}